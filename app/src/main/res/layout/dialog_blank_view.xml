<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/msg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/redD5"
        android:textSize="@dimen/text_12_size"
        android:text="@string/app_name"
        android:padding="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_5"
        />
    <TextView
        android:id="@+id/blank_user_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:textSize="@dimen/text_14_size"
        android:textColor="@color/black3"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="@string/app_name"
        />

    <TextView
        android:id="@+id/blank_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:textSize="@dimen/text_14_size"
        android:textColor="@color/black3"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="@string/app_name"
        />

    <TextView
        android:id="@+id/blank_city"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:textSize="@dimen/text_14_size"
        android:textColor="@color/black3"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="@string/app_name"
        />

    <TextView
        android:id="@+id/blank_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:textSize="@dimen/text_14_size"
        android:textColor="@color/black3"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="@string/app_name"
        />

    <TextView
        android:id="@+id/blank_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_10"
        android:textSize="@dimen/text_14_size"
        android:textColor="@color/black3"
        android:layout_marginLeft="@dimen/dp_5"
        android:text="@string/app_name"
        />



</LinearLayout>